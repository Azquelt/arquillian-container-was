<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/arquillian http://jboss.org/schema/arquillian/arquillian-1.0.xsd">

	<engine>
		<property name="deploymentExportPath">target/</property>
	</engine>

	<container qualifier="websphere" default="true">
		<configuration>
			<property name="remoteServerAddress">localhost</property>
			<property name="remoteServerSoapPort">8880</property>
			<property name="remoteServerHttpPort">9080</property>
		</configuration>
	</container>
</arquillian>
